{
  "name": "animated-vue",
  "version": "0.5.1",
  "description": "A set of transitions for Vue2 based on animate.css",
  "main": "dist/animated-vue.js",
  "scripts": {
    "dev": "babel-node build/server/index.js --presets es2015,stage-0",
    "build:docs": "babel-node build/server/build-docs.js --presets es2015,stage-0",
    "build:lib": "babel-node build/server/build-lib.js --presets es2015,stage-0",
    "build": "rm -rf dist && npm run build:lib",
    "final-build": "rm -rf dist && npm run build:docs && npm run build:lib",
    "lint": "eslint --ext .js,.vue --fix src docs",
    "release": "bash build/release.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/radical-dreamers/animated-vue.git"
  },
  "keywords": [
    "Vue",
    "animate.css",
    "es6",
    "javascript",
    "Vue2",
    "transitions"
  ],
  "author": "Rodrigo Juliani <srodriki@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/radical-dreamers/animated-vue/issues"
  },
  "homepage": "https://github.com/radical-dreamers/animated-vue#readme",
  "devDependencies": {
    "autoprefixer": "^6.7.4",
    "autosize": "^3.0.20",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-stage-0": "^6.22.0",
    "chalk": "^1.1.3",
    "chokidar": "^1.6.1",
    "clipboard": "^1.6.0",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "css-mqpacker": "^5.0.1",
    "date-fns": "^1.27.2",
    "element.scrollintoviewifneeded-polyfill": "^1.0.1",
    "es6-promise": "^4.1.0",
    "eslint": "^3.16.0",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^2.0.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.0",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "highlight.js": "^9.9.0",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.3",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "optimize-js-plugin": "^0.0.4",
    "ora": "^1.1.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.5",
    "url-loader": "^0.5.7",
    "vue-hot-reload-api": "^2.1.0",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^12.2.1",
